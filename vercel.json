{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(\\d+)/(\\d+)\\.html", "destination": "/static/$1/$2.html" },
    { "source": "/(.*)", "destination": "/" }
  ]
}